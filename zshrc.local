# Path to your oh-my-zsh configuration
ZSH=$HOME/.oh-my-zsh

# ZSH Theme
ZSH_THEME="muse"

# Which plugins would you like to load?
plugins=(git cap brew rails3 bundler vagrant)

# Use oh-my-zsh ZSH
source $ZSH/oh-my-zsh.sh

# RBENV
eval "$(rbenv init -)"

# fix for windown naming issue with tmuxinator
export DISABLE_AUTO_TITLE=true
